{
    "java.configuration.updateBuildConfiguration": "interactive",
    "java.test.config": [
        {
            "name": "junit5",
            "workingDirectory": "${workspaceFolder}/Backend",
            "classpaths": [
                "${workspaceFolder}/Backend/target/test-classes",
                "${workspaceFolder}/Backend/target/classes"
            ],
            "modulepaths": [],
            "vmargs": [
                "-Dspring.profiles.active=test"
            ]
        }
    ],
    "java.import.maven.enabled": true,
    "java.project.sourcePaths": [
        "Backend/src/main/java",
        "Backend/src/test/java"
    ],
    "java.project.outputPath": "Backend/target",
    "maven.terminal.useJavaHome": true,
    "postman.settings.dotenv-detection-notification-visibility": false,
    "maven.view": "hierarchical",
    "java.test.defaultConfig": "junit5",
    "java.debug.settings.enableRunDebugCodeLens": true,
    "java.test.message.enableDecoration": true,
    "java.test.forceBuildBeforeTest": true,
    "java.compile.nullAnalysis.mode": "automatic",
    "java.test.runner.enabled": true,
    "java.test.report.position": "sideView",
    "livePreview.defaultPreviewPath": "/Backend/target/site/jacoco/index.html"
}